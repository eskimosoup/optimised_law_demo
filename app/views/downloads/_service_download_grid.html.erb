<div class="medium-4 columns <%= 'end' if download_presenter == @presented_all_downloads.last %>">
  <div class="service-download">
    <div class="row">
      <div class="medium-4 columns">
        <div class="service-download-image">
          <%= download_presenter.index_image %>
        </div>
      </div>

      <div class="medium-8 columns">
        <div class="service-download-content">
          <div class="service-download-title">
            <%= download_presenter.title %>
          </div>

          <div class="service-download-overview">
            <%= download_presenter.summary %>
            <%= download_presenter.download_button %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
